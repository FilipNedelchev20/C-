using System;

namespace 2SumDigits
{
    class Program
    {
        static void Main(string[] args)
        {
            int numbers = int.Parse(Console.ReadLine());
            int sumOfDigits = 0;
            while (numbers != 0)
            {
                sumOfDigits = sumOfDigits + (numbers % 10);
                numbers = numbers / 10;

            }
            Console.WriteLine(sumOfDigits);

        }
    }
}
