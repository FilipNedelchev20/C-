using System;

namespace 3MatrixOfIntegers
{
    class Program
    {

        // въвеждаме първи метод за колони и редове
        private static void Matrix(int number)
        {

            // въвеждаме първият цикъл
            for (int i = 0; i < number; i++)
            {//въвеждаме вторият цикъл
                for (int O = 0; O < number; O++)
                {//изписваме на конзолата първият ред
                    Console.Write(number + " ");
                }
                // пишем за да може като приключи горе да продължи на нов ред
                Console.WriteLine();
            }


        }

            static void Main(string[] args)
            {// въвеждаш променливата чилсото
                int number = int.Parse(Console.ReadLine());
           // извикваме метода
                Matrix(number);
            }
        
    }
}
