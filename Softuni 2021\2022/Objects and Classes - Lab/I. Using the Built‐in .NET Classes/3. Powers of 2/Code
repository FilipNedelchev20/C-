using System.Numerics;
using System;
using System.Collections.Generic;
using System.Linq;


namespace 3PowersOf2
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            BigInteger result = 1;

            for (int i= 0;i < n; i++)

            {
                result *= 2;
            }
            Console.WriteLine(result);
        }
    }
}
