using System;
using System.Collections.Generic;
using System.Linq;


namespace 2ExtractFile
{
    class Program
    {
        static void Main(string[] args)
        {
            List<string> name = Console.ReadLine().Split('\\').ToList();
            string[] extension = name[name.Count - 1].Split('.');
            Console.WriteLine($"File name: {extension[0]}");
            Console.WriteLine($"File extension: {extension[1]}");

        }
    }
}
