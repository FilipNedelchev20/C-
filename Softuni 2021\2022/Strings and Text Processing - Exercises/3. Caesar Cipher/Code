using System;


namespace 3CeasarCipher
{
    class Program
    {
        static void Main(string[] args)
        {
            string text = Console.ReadLine();
            string chipper = string.Empty;
            for (int i = 0; i < text.Length; i++)
            {
                char letter = text[i];
                letter += (char)3;
                chipper += letter;
            }
            Console.WriteLine(chipper);
        }
    }
}
